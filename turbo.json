{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": [],
      "cache": false
    },
   "dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    }
  }
}
